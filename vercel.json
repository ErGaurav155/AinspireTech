{
  "functions": {
    "app/**/*": {
      "memory": 1024,
      "maxDuration": 60
    },
    "app/WebsiteOnboarding": {
      "includeFiles": "node_modules/chrome-aws-lambda/**/*"
    }
  },
  "headers": [
    {
      "source": "/widget-loader.js",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors *;"
        }
      ]
    },
    {
      "source": "/widget.js",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors *;"
        }
      ]
    },
    {
      "source": "/ChatBots",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors *;"
        }
      ]
    },
    {
      "source": "/widget.bundle.js",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors *;"
        }
      ]
    }
  ]
}
